<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.itx.mapper.SpCategoryMapper">

    <resultMap id="BaseResultMap" type="com.itx.pojo.SpCategory">
            <id property="catId" column="cat_id" jdbcType="INTEGER"/>
            <result property="catName" column="cat_name" jdbcType="VARCHAR"/>
            <result property="catPid" column="cat_pid" jdbcType="INTEGER"/>
            <result property="catLevel" column="cat_level" jdbcType="INTEGER"/>
            <result property="catDeleted" column="cat_deleted" jdbcType="INTEGER"/>
            <result property="catIcon" column="cat_icon" jdbcType="VARCHAR"/>
            <result property="catSrc" column="cat_src" jdbcType="VARCHAR"/>
    </resultMap>

    <sql id="Base_Column_List">
        cat_id,cat_name,cat_pid,
        cat_level,cat_deleted,cat_icon,
        cat_src
    </sql>
</mapper>
